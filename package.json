{
   "name": "@dhruwlalan/quickrepo",
   "version": "1.0.0",
   "description": "A single command to quickly create a repository and host it on github.",
   "main": "lib/index.js",
   "bin": {
      "qr": "bin/quickrepo.js"
   },
   "engines": {
      "node": ">=10"
   },
   "files": [
      "bin",
      "lib"
   ],
   "publishConfig": {
      "access": "public"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/dhruwlalan/quickrepo.git"
   },
   "keywords": [
      "commander",
      "inquirer",
      "cli",
      "utility",
      "octokit",
      "git",
      "github"
   ],
   "author": "dhruw lalan <dhruwlalan22@gmail.com>",
   "license": "MIT",
   "bugs": {
      "url": "https://github.com/dhruwlalan/quickrepo/issues"
   },
   "homepage": "https://github.com/dhruwlalan/quickrepo#readme",
   "scripts": {
      "prepare": "types-sync",
      "prepublishOnly": "rimraf dist && npm run build",
      "start": "babel --watch src --out-dir lib --extensions \".ts\"",
      "build": "babel src --out-dir lib --extensions \".ts\""
   },
   "dependencies": {
      "@dhruwlalan/semantic-release-config": "^2.0.1",
      "@octokit/rest": "^18.5.3",
      "colorette": "^1.2.2",
      "commander": "^7.2.0",
      "configstore": "^5.0.1",
      "execa": "^5.0.0",
      "inquirer": "^8.0.0",
      "ora": "^5.4.0",
      "semantic-release": "^19.0.2",
      "update-notifier": "^5.1.0"
   },
   "devDependencies": {
      "@babel/cli": "^7.13.16",
      "@babel/core": "^7.14.2",
      "@babel/preset-env": "^7.14.2",
      "@babel/preset-typescript": "^7.13.0",
      "@types/configstore": "^5.0.0",
      "@types/inquirer": "^7.3.1",
      "@types/node": "^15.0.1",
      "@types/semantic-release": "^17.2.3",
      "@types/update-notifier": "^5.0.0",
      "@typescript-eslint/eslint-plugin": "^4.23.0",
      "@typescript-eslint/parser": "^4.23.0",
      "eslint": "^7.24.0",
      "eslint-config-prettier": "^8.3.0",
      "eslint-plugin-prettier": "^3.4.0",
      "prettier": "^2.2.1",
      "rimraf": "^3.0.2",
      "types-sync": "^2.0.3",
      "typescript": "^4.2.4"
   }
}
